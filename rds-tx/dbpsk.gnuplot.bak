reset
input_file = 'lookuptable.csv'

# every 1::<first>::<last>

# positive half-sine:    32::47  (x-32, 128-y)
# negative half-sine:    32::47  (x-32,     y)
# positive phase change:  0::47  (x   ,     y)
# negative phase change:  0::47  (x   , 128-y)

plot \
   input_file every 1::32::47 using (16+$1):(128-$2), \
   input_file every 1::32::47 using (32+$1):($2), \
   input_file every 1::0::47  using (80+$1):($2), \
   input_file every 1::0::47  using (128+$1):($2), \
   input_file every 1::32::47 using (144+$1):(128-$2), \
   input_file every 1::0::47  using (192+$1):(128-$2), \

pause -1
